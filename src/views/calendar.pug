extends layout.pug

block content
  h1 Calendar
  .calendar-container(style="position: relative; padding-bottom: 75%; height: 0; overflow: hidden; max-width: 100%;")
    iframe(style="position: absolute; top: 0; left: 0; width: 100%; height: 100%; border: 0;",
           src=${config.calURL},
           frameborder="0",
           scrolling="no")
